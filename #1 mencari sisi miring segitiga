#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    double tinggi,alas;
	float AC;
    cout<<"masukan nilai tinggi =";
    cin>>tinggi;
    cout<<"masukan nilai alas =";
    cin>>alas;
    AC=(tinggi*tinggi)+(alas*alas);
    AC=sqrt(AC);
    cout<<"panjang sisi miring ="<<AC<<endl;
    
}  
